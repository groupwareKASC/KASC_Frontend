{
  "name": "근태 검증 프로그램",
  "main": "build/electron.js",
  "homepage": "./",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mynaui/icons-react": "0.3.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "concurrently": "9.2.0",
    "electron-is-dev": "3.0.1",
    "framer-motion": "12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "7.8.1",
    "react-scripts": "5.0.1",
    "styled-components": "6.1.19",
    "typescript": "^4.4.2",
    "wait-on": "8.0.4",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "9.1.2",
    "@storybook/addon-onboarding": "9.1.2",
    "@storybook/preset-create-react-app": "9.1.2",
    "@storybook/react-webpack5": "9.1.2",
    "@types/styled-components": "5.1.34",
    "electron": "37.3.1",
    "electron-builder": "26.0.12",
    "eslint-plugin-storybook": "9.1.2",
    "prop-types": "15.8.1",
    "storybook": "9.1.2",
    "webpack": "5.101.3"
  },
  "scripts": {
    "start": "concurrently \"set BROWSER=none && set PORT=13000 && react-scripts start\" \"wait-on http://localhost:13000 && electron ..\"",
    "build": "react-scripts build",
    "electron": "electron .",
    "pack": "npm run build && electron-builder build -c.extraMetadata.main=build/electron.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis",
      "signAndEditExecutable": false
    }
  }
}
